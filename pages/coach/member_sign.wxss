/* pages/coach/member_sign.wxss */

.bg-box {
  width: 697rpx;
  margin: 0 auto;
  background: #ffab18;
  position: relative;
}

.mask-bg {
  width: 750rpx;
  position: absolute;
  right: -55rpx;
  bottom: -220rpx;
  opacity: 0.05;
  z-index: -1;
  transform: rotate(-15deg);
}

.circular {
  position: absolute;
  bottom: -15rpx;
  left: -15rpx;
  width: 30rpx;
  height: 30rpx;
  background: #1f1f1f;
  border-radius: 50%;
  z-index: 1;
}

.circular1 {
  position: absolute;
  bottom: -15rpx;
  right: -15rpx;
  width: 30rpx;
  height: 30rpx;
  background: #1f1f1f;
  border-radius: 50%;
  z-index: 1;
}

/**课程 二维码**/

.list-box {
  padding: 50rpx 30rpx;
  text-align: center;
  position: relative;
}

.logo-text {
  position: absolute;
  top: 45rpx;
  left: 50%;
  margin-left: -215rpx;
  width: 430.7rpx;
  height: 66.7rpx;
}

.title {
  position: relative;
  z-index: 1;
  font-weight: bold;
  font-size: 40rpx;
}

.date-time {
  margin-top: 10rpx;
  font-size: 29.3rpx;
}

/**签到扫码**/

.qrcode {
  width: 286.7rpx;
  height: 286.7rpx;
  margin-top: 80rpx;
}

.tips-box {
  font-size: 29.3rpx;
  margin-top: 80rpx;
  padding-bottom: 80rpx;
  border-bottom: solid 1.3rpx #d75600;
}

.s-tips-box {
  margin: 0;
  padding-bottom: 20rpx;
}

.tips-text {
  margin-top: 15rpx;
}

.btn-box {
  width: 500rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  margin-top: 15rpx;
  justify-content: space-between;
}

.tips-btn {
  width: 186rpx;
  height: 73rpx;
  line-height: 73rpx;
  border-radius: 7rpx;
  font-size: 33rpx;
  font-weight: bold;
  border: solid 2rpx #000;
}

.ok-text {
  color: #000;
  font-size: 40rpx;
  font-weight: bold;
  margin-top: 15rpx;
}

/**课程收入**/

.course-income {
  height: 600rpx;
}

.course-tips {
  color: #000;
  font-size: 32rpx;
  margin-top: 50rpx;
}

.course-rwo {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
}

.course-text {
  font-size: 28rpx;
  color: #000;
}

.line {
  border-bottom: solid 1.3rpx #d75600;
}

.branch-text {
  color: #000;
  font-size: 32rpx;
}

/**学员**/

.student {
  position: relative;
  overflow: hidden;
  background: #ffab18;
  z-index: 1;
}

.student-title-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx 20rpx;
}

.student-tips {
  font-size: 28rpx;
  color: #000;
}

.scroll-box {
  width: 697rpx;
  height: 450rpx;
  margin: 0 auto;
}

.student-box {
  height: 500rpx;
}

.student-portrait {
  display: flex;
  flex-wrap: wrap;
}

.portrait-box {
  width: 138rpx;
  text-align: center;
  margin-bottom: 35rpx;
  position: relative;
}

.head-portrait {
  width: 105.3rpx;
  height: 105.3rpx;
  border-radius: 5.3rpx;
  position: relative;
}

.student-name {
  font-size: 20rpx;
  color: #000;
  overflow: hidden;
  white-space: nowrap;
  margin-top: 10rpx;
  text-overflow: ellipsis;
}

.mask {
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -52.7rpx;
  width: 105.3rpx;
  height: 105.3rpx;
  border-radius: 5.3rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0, 0, 0, 0.6);
}

.hook-icon {
  width: 48rpx;
  height: 34.7rpx;
}

.number {
  width: 36.7rpx;
  height: 36.7rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 1;
  color: #000;
  line-height: 36.7rpx;
  background-color: #fff;
  border-radius: 50%;
  text-align: center;
  box-shadow: 0rpx 0rpx 3.3rpx 0rpx rgba(0, 0, 0, 0.6);
}

/**课程**/

.curriculum-box {
  width: 637rpx;
  margin: 0 auto;
  padding: 0 30rpx 50rpx;
  background: #fffbe6;
  border-bottom-left-radius: 13rpx;
  border-bottom-right-radius: 13rpx;
}

.row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  border-bottom: 1.3rpx solid #a5a5a5;
}

.name-icon-box {
  display: flex;
  align-items: center;
}

.icon {
  width: 31.3rpx;
  height: 32rpx;
}

.curriculum-btn {
  width: 99.3rpx;
  height: 52.7rpx;
  line-height: 52.7rpx;
  text-align: center;
  font-size: 26.7rpx;
  border-radius: 6.7rpx;
  border: solid 2rpx #848484;
  color: #848484;
}

.row-address {
  padding: 30rpx 0;
  border-bottom: 1.3rpx solid #a5a5a5;
}

.address-icon {
  width: 35rpx;
  height: 35rpx;
}

.address {
  padding: 20rpx 0;
  color: #676767;
}

.map-text {
  color: #000;
  text-decoration: underline;
  font-weight: 500;
}

.info-row {
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1.3rpx solid #a5a5a5;
}

.row-text {
  color: #000;
  font-weight: 500;
  margin-left: 15rpx;
}

.curriculum-row {
  display: flex;
  align-items: center;
  padding: 30rpx 0;
}

.course-img-box {
  display: flex;
  flex-wrap: wrap;
}

.curriculum-img-box {
  text-align: center;
}
.up-tips{
  width: 140rpx;
  height: 140rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 22rpx;
  margin-right: 24rpx;
  margin-bottom: 24rpx;
  background: rgba(0, 0, 0, 0.5);
}
.play{
  width: 45rpx;
  height: 45rpx;
}
.course-img {
  width: 140rpx;
  height: 140rpx;
  margin-right: 24rpx;
  margin-bottom: 24rpx;
}

.course-img:nth-child(4n+0) {
  margin-right: 0;
}

.img-text {
  color: #848484;
  font-size: 24rpx;
}

.save-btn {
  border: 1px solid #333;
  color: #848484;
  margin: 30rpx 0;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  text-align: center;
}

.bg {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0, 0, 0, 0.5);
}

/**查看新学员**/

.container-box {
  width: 650rpx;
  background: #fff;
  border-radius: 13rpx;
  text-align: right;
}

.close-icon {
  width: 35rpx;
  height: 35rpx;
  margin: 15rpx;
}

.container-row-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 25rpx;
  margin: 0 20rpx;
  border-bottom: 1px solid #a5a5a5;
}

.container-left {
  flex: 1;
  text-align: left;
}

.container-tips {
  color: #a5a5a5;
  font-size: 20rpx;
}

.container-row-box:last-child {
  border: 0;
}

.number-tips {
  display: inline-block;
  font-size: 24rpx;
}

/**训练营扫码**/

.camp-mask-box {
  width: 590rpx;
  background-color: #fffbe6;
  border-radius: 5rpx;
  position: relative;
}

.camp-title {
  font-size: 32rpx;
  text-align: center;
  padding: 30rpx 0;
  color: #000;
}

.camp-qrcode {
  text-align: center;
  margin-bottom: 120rpx;
}

.camp-qrcode image {
  width: 286.7rpx;
  height: 286.7rpx;
}

/**不接单**/

.mask-text-box {
  width: 590rpx;
  height: 333rpx;
  background-color: #fffbe6;
  border-radius: 5rpx;
  position: relative;
}

.mask-text-title {
  font-size: 32rpx;
  text-align: center;
  padding: 30rpx 0;
  color: #000;
}

.input-box {
  width: 459rpx;
  height: 73rpx;
  margin: 0 auto;
  line-height: 73rpx;
  background-image: linear-gradient(#fff, #fff);
  border-radius: 7rpx;
  border: solid 2rpx #5e5e5e;
  padding-left: 10rpx;
}

.input-box input {
  height: 100%;
  width: 100%;
}

.mask-btn-box {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  align-items: center;
  height: 108rpx;
  line-height: 108rpx;
}

.mask-btn {
  flex: 1;
  text-align: center;
  font-size: 32rpx;
  color: #000;
  border-top: 2rpx solid #a5a5a5;
}

.confirm-btn {
  flex: 1;
  text-align: center;
  font-size: 32rpx;
  color: #000;
  border-top: 2rpx solid #ffbc0f;
  background: url(https://boringcdn.nanningboring.com/molicheng/ranking-btn.png);
  background-size: 100% 100%;
}

.s-btn {
  border: 1px solid #333;
  margin: 50rpx 0;
  color: #848484;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  text-align: center;
}



.mask-text-tips {
  width: 447rpx;
  margin: 20rpx auto 0;
  text-align: center;
  line-height: 40rpx;
  color: #676767;
  font-size: 28rpx;
}

/**魔橙圈**/
.msg-board {
  border-top: 1px solid #a5a5a5;
  margin-top: 40rpx;
}

.msg-list {
  height: 460rpx;
  padding-bottom: 30rpx;
}

.msg-row {
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.user-icon {
  width: 53rpx;
  height: 53rpx;
  border-radius: 5rpx;
}

.msg-info {
  flex: 1;
  margin-left: 15rpx;
}

.date-box {
  display: flex;
  align-items: center;
  font-size: 20rpx;
}

.star-row {
  display: flex;
  align-items: center;
  margin-left: 10rpx;
}

.star {
  width: 25rpx;
  height: 24rpx;
  background: url(https://boringcdn.nanningboring.com/molic/large-star.png);
  background-size: 100%;
  margin-right: 5rpx;
  margin-top: -3rpx;
}

.light-star {
  background: url(https://boringcdn.nanningboring.com/molic/large-light.png);
  background-size: 100%;
}

.info {
  font-size: 24rpx;
  color: #000;
}
.no-data-tips{
  text-align: center;
  color: #999;
  font-size: 26rpx;
  margin-top: 100rpx;
}

.ipt-box {
  height: 58rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
  background-color: #ffffff;
  border-radius: 5rpx;
  border: solid 1rpx #9e9e9e;
}

.ipt {
  flex: 1;
  padding-left: 10rpx;
  height: 100%;
  font-size: 26rpx;
}

.ipt-btn {
  width: 128rpx;
  height: 57rpx;
  line-height: 57rpx;
  text-align: center;
  background-image: linear-gradient(0deg,
      #ff6e0d 0%,
      #ffba00 100%),
    linear-gradient(#ffffff,
      #ffffff);
  border-radius: 5rpx;
  border-left: solid 1rpx #9e9e9e;
  font-weight: 600;
}

/**选择上传类型**/
.mask-box{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 1;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  align-items: flex-end;
}
.opt-type-box{
  position: relative;
  bottom: -150px;
  width: 100%;
  background: #fff;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}
.opt-type-text{
  text-align: center;
  border-bottom: 1px solid #eee;
  padding: 25rpx 30rpx;
}

.cancel-text{
  border-top: 3px solid #eee;
  padding: 25rpx 30rpx;
  text-align: center;
}