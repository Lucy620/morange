/* pages/league/list.wxss */

.wrapper-box{
  position: fixed;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%
}

.container-box {
  z-index: 1;
  
}

/***轮播**/

.swiper-box {
  height: 200rpx;
}

swiper {
  height: 100%;
}

.slide-image {
  width: 100%;
}

/***tab栏**/

.tab-box {
  display: flex;
  align-items: center;
  height: 70rpx;
  background-color: #181818;
  border-bottom: 1px solid #3d3d3d;
}

.row-box {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  border-right: 1rpx solid #272727;
}

.row-box:last-child {
  border: none;
}

.title {
  font-size: 24rpx;
  color: #fff;
}

.tab-icon {
  width: 25rpx;
  height: 25rpx;
  margin-left: 10rpx;
}

/**星期**/

.week-box {
  height: 62rpx;
  line-height: 62rpx;
  background-color: #181818;
  display: flex;
  align-items: center;
}

.week-item {
  flex: 1;
  text-align: center;
  color: #818181;
  font-size: 24rpx;
  position: relative;
}

.s-line {
  position: relative;
}

.s-line::after {
  content: '';
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -25rpx;
  width: 1px;
  height: 50rpx;
  background: #272727;
}

.select-week-box {
  position: absolute;
  top: -7rpx;
  left: 50%;
  margin-left: -33rpx;
  width: 66rpx;
  height: 66rpx;
  z-index: 1;
  border-radius: 50%;
  background-image: linear-gradient(to bottom, #ffd324, #ff7200);
  padding: 5rpx;
}

.select-week {
  width: 66rpx;
  height: 66rpx;
  background-color: #252525;
  border-radius: 50%;
}

.select-date {
  padding-top: 12rpx;
  width: 100%;
  font-size: 20rpx;
  color: #818181;
  text-align: center;
  line-height: 1.2;
}

.select-date-tips {
  width: 100%;
  font-size: 22rpx;
  color: #fff;
  text-align: center;
  line-height: 1.2;
}

/**教练 门店**/

.coach-store-box {
